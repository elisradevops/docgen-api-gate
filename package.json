{
  "name": "dg-api-gate",
  "version": "2.19.0",
  "description": "a document generator api gate",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "start": "node server.js",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest --coverage --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/msal-node": "^2.6.0",
    "axios": "^0.18.1",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "httpntlm": "^1.7.7",
    "jaeger-client": "^3.18.1",
    "minio": "^7.0.26",
    "moment": "^2.25.3",
    "mongoose": "^5.13.23",
    "multer": "^1.4.5-lts.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.0",
    "@types/nodemailer": "^6.4.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-util": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/bin/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/test/**",
      "!src/**/__mocks__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "output": "./junit.xml"
  }
}
